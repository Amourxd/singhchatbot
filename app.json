{
    "name": "ArnavChatBot",
    "description": "Hello, I am Arnav An Intelligent ChatBot. If You Are Feeling Lonely, You can Always Come to me and Chat With Me!",
    "logo": "https://telegra.ph/file/bdaed2547032396ad8f56.jpg",
    "repository": "https://github.com/Amourxd/singhchatbot",
    "env": {
        "STRING": {
            "description": "Pyrogram String Session here",
            "required": true
        },
     
        "MONGO_URL": {
            "description": "mongo db url here",
            "required": true
        },
        "API_ID": {
            "description": "telegram api id here",
            "required": true
        },
        "API_HASH": {
            "description": "telegram api hash here",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
